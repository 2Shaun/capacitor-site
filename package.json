{
  "name": "capacitor-site",
  "private": true,
  "version": "0.0.1",
  "description": "Capacitor Site",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "lint": "npm run prettier -- --check",
    "fmt": "npm run prettier -- --write",
    "prettier": "prettier \"{blog,docs,src}/**/*.{css,scss,html,ts,tsx,md}\"",
    "docs.plugins": "ts-node --project scripts/tsconfig.json scripts/plugin-api.ts",
    "docs.plugins.v2": "ts-node --project scripts/tsconfig.json scripts/plugin-api-v2.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@capacitor/core": "^2.4.2",
    "@capacitor/docgen": "^0.0.10",
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/plugin-ideal-image": "^2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@ionic-internal/docusaurus-theme": "^1.1.2",
    "@ionic/prettier-config": "^1.0.1",
    "@mdx-js/react": "^1.6.21",
    "@types/node-fetch": "^2.5.7",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "lqip": "^2.1.0",
    "lqip.macro": "^0.1.5",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^2.7.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-ideal-image": "0.0.5",
    "sass": "^1.32.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/capacitor"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "homepage": "https://github.com/ionic-team/capacitor",
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@ionic/prettier-config"
}
